<!DOCTYPE html>
<html>
  <head>
    <title>Bolt</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header id="main-header">
      <div class="container">
        <h1>Bolt</h1>
      </div>
    </header>

    <nav id="navbar">
      <div class="container">
        <ul>
          <li>
          <%= link_to "Home", home_path %>
          </li>
          <li>
          <%= link_to "Membership", membership_path %>
          </li>
          <li>
          <%= link_to "Gyms Near Me", gyms_path %>
          </li>
          <li>
          <%= link_to "Classes", run_classes_path %>
          </li>
          <li>
          <%= link_to "Training", training_path %>
          </li>
          <li>
          <%= link_to "Virtual", virtual_path %>
          </li>
          <li>
          <%= link_to "Coaches", run_sessions_path %>
          </li>
          <li>
          <%= link_to "Login", login_path %>
          </li>
          <li>
            <%= link_to "Join Now", new_member_path %>
          </li>
        </ul>
      </div>
    </nav>
    


    <%= yield %>

    <footer id="main-footer">
      <p class=text-muted">&copy; 2021 Bolt Coportation. All rights reserved. Bolt Copyright Policy<p>
    </footer>
    </div>
  </body>
</html>
